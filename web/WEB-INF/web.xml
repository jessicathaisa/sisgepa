<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RelatorioServlet</servlet-name>
        <servlet-class>com.jessica.Servlet.RelatorioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServlet</servlet-name>
        <servlet-class>com.jessica.Servlet.UsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfessorServlet</servlet-name>
        <servlet-class>com.jessica.Servlet.ProfessorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisadorServlet</servlet-name>
        <servlet-class>com.jessica.Servlet.PesquisadorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlunoServlet</servlet-name>
        <servlet-class>com.jessica.Servlet.AlunoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrientacaoServlet</servlet-name>
        <servlet-class>com.jessica.Servlet.OrientacaoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RelatorioServlet</servlet-name>
        <url-pattern>/RelatorioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/UsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisadorServlet</servlet-name>
        <url-pattern>/PesquisadorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfessorServlet</servlet-name>
        <url-pattern>/ProfessorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlunoServlet</servlet-name>
        <url-pattern>/AlunoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrientacaoServlet</servlet-name>
        <url-pattern>/OrientacaoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
